{
  "name": "express_base_template",
  "version": "0.0.1",
  "description": "express base template",
  "main": "server.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/server",
    "start": "NODE_ENV=production node src/server",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "joi": "^17.5.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.13.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JhonAngulo/express_base_template.git"
  },
  "author": "Jhon Manuel Angulo Moncada <jhonmanuelm89@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JhonAngulo/express_base_template/issues"
  },
  "homepage": "https://github.com/JhonAngulo/express_base_template#readme"
}
